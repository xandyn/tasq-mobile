{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "jest": true,
    "browser": true,
    "mocha": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ios.js", ".android.js"]
      }
    }
  },
  "rules": {
    "global-require": 0,
    "new-cap": 0,
    "no-return-await": 0,
    "no-unused-vars": [1, { "varsIgnorePattern": "(bindActionCreators|connect|colors|chalk|dotenv|PropTypes)", "args": "none" }],
    "import/no-extraneous-dependencies": 0,
    "consistent-return": 0,
    "no-prototype-builtins": 0,
    "no-confusing-arrow": 0,
    "no-constant-condition": 0,
    "comma-dangle": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "no-multi-str": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/anchor-has-content": 0,
    "no-mixed-operators": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-nested-ternary": 0
  },
  "plugins": [
    "react",
    "react-native"
  ]
}
